- const jumbotronTitle = require("01-elements/title/index.pug");
- const jumbotronD = require("03-sections/jumbotron/data/index.json");
- const jumbotronS = require("03-sections/jumbotron/styles/index.json");

mixin jumbotron(props = {})
  section(class=jumbotronS.section)
    div(class=jumbotronS.main)
      .container
        div(class=jumbotronS.wrapper)
          div(class=jumbotronS.icon)
            img(
              class=jumbotronS.img
              src=require("03-sections/jumbotron/static/logo.png")
              alt=jumbotronD.iconAlt
            )
          div(class=jumbotronS.title)!= jumbotronTitle(props ? { props } : { props: { text: jumbotronD.title } })
